<html>
	<title>File uploads</title>
	<body>
		<h2>File uploads</h2>
		<form method="POST" enctype="multipart/form-data" action="">
			File to upload: <input type="file" name="fileup"><br>
			<input type="submit" value="Upload files now.">
		</form>

		<h3>Files:</h3>
		{{#files}}{{name}}<br>{{/files}}
        
        
        <br><br><br>
        <h3>Download File:</h3>
        <select class="selectpicker">
            {{#files}}
            <option>{{name}}</option>
            {{/files}}
        </select>


	</body>
</html>
